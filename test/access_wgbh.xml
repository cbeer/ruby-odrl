<?xml version="1.0" encoding="UTF-8"?>
<o-ex:rights  xmlns:o-ex="http://odrl.net/1.1/ODRL-EX" xmlns:o-dd="http://odrl.net/1.1/ODRL-DD">
    <o-ex:offer>
        <o-ex:asset>
            <o-ex:context>
                <o-dd:dLocation>.*</o-dd:dLocation>
            </o-ex:context>
        </o-ex:asset>
        <o-ex:permission>
            <o-dd:play>
                <o-ex:requirement>
                    <o-dd:register />
                </o-ex:requirement>
            </o-dd:play>
	    <o-dd:annotate>
                <o-ex:requirement>
                    <o-dd:register />
                </o-ex:requirement>
	    </o-dd:annotate>
            <o-dd:save>
                <o-ex:constraint>
			<o-dd:individual>
				<o-ex:context>
					<o-dd:name>wgbh.org$</o-dd:name>
				</o-ex:context>
			</o-dd:individual>
                </o-ex:constraint>
            </o-dd:save>
        </o-ex:permission>
    </o-ex:offer>
</o-ex:rights>
